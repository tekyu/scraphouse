{
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "src",
    "paths": {
      "assets/*": ["assets/*"],
      "store/*": ["store/*"],
      "components/*": ["components/*"],
      "containers/*": ["containers/*"],
      "utils/*": ["utils/*"],
      "theme/*": ["theme/*"],
      "i18n/*": ["i18n/*"],
      "mocks/*": ["mocks/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
